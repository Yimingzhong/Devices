<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties resource="db.properties">
    </properties>
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <!--    给实体类起别名-->
    <typeAliases>
        <package name="com.ding.pojo"/>
    </typeAliases>
    <mappers>
        <mapper resource="mapper/xml/ChargeMapper.xml"/>
        <mapper resource="mapper/xml/CustomerMapper.xml"/>
        <mapper resource="mapper/xml/DeviceMapper.xml"/>
        <mapper resource="mapper/xml/PartFlowMapper.xml"/>
        <mapper resource="mapper/xml/PartMapper.xml"/>
        <mapper resource="mapper/xml/PartRequireMapper.xml"/>
        <mapper resource="mapper/xml/RepairRecordMapper.xml"/>
        <mapper resource="mapper/xml/ServiceRecordMapper.xml"/>
        <mapper resource="mapper/xml/UserMapper.xml"/>
    </mappers>
</configuration>